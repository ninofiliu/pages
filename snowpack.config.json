{
  "mount": {
    "public": {
      "url": "/",
      "static": true,
      "resolve": false
    },
    "src": {
      "url": "/dist"
    }
  },
  "plugins": [
    "@snowpack/plugin-sass",
    "@snowpack/plugin-typescript"
  ],
  "routes": [
    {
      "match": "routes",
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
